    <ProjectName Condition="'$(ProjectName)' == ''">$(MSBuildProjectName)</ProjectName>
    <DocumentationFile>$(OutputPath)\$(ProjectName).xml</DocumentationFile>
    <XlfLanguages>en;$(XlfLanguages)</XlfLanguages>
