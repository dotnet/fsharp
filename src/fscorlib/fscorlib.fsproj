<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>

    <NoWarn>$(NoWarn);62</NoWarn>       <!-- ML Compat -->
    <NoWarn>$(NoWarn);75</NoWarn>       <!-- InternalCommandLineOption -->
    <NoWarn>$(NoWarn);1204</NoWarn>     <!-- This construct is for use in the FSharp.Core library and should not be used directly -->
    <NoWarn>$(NoWarn);57</NoWarn>       <!-- Turn off 57: Use of construct with Experimental attribute -->
    <NoWarn>$(NoWarn);3511</NoWarn>     <!-- Turn off 3511: state machine not compilable - expected for inlined functions defining state machine generators -->
    <NoWarn>$(NoWarn);3513</NoWarn>     <!-- Turn off 3513: resumable code invocation' - expected for resumable code combinators -->

    <AllowCrossTargeting>true</AllowCrossTargeting>
    <DefineConstants>$(DefineConstants);FSHARP_CORE</DefineConstants>
    <OtherFlags>$(OtherFlags) --warnon:3218</OtherFlags>          <!-- 3218: ArgumentsInSigAndImplMismatch -->
    <OtherFlags>$(OtherFlags) --warnon:1182</OtherFlags>          <!-- 1182: Unused variables -->
    <OtherFlags>$(OtherFlags) --warnon:3390</OtherFlags>          <!-- 3390: xmlDocBadlyFormed -->
    <OtherFlags>$(OtherFlags) --warnon:3520</OtherFlags>          <!-- 3520: invalidXmlDocPosition -->
    <OtherFlags>$(OtherFlags) --compiling-fscorlib --compiling-fslib  --maxerrors:100 --extraoptimizationloops:1</OtherFlags>
    <Tailcalls>true</Tailcalls>            <!-- .tail annotations always emitted for this binary, even in debug mode -->
    <NGenBinary>true</NGenBinary>
    <PreRelease>true</PreRelease>
    <Configurations>Debug;Release;Proto</Configurations>
  </PropertyGroup>

