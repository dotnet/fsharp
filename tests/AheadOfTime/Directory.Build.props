<Project>
  <!-- Define FSharpNetCoreProductDefaultTargetFramework directly by reading from the single source of truth.
       We can't import root Directory.Build.props because MSBuildThisFileDirectory would be wrong. -->
  <PropertyGroup>
    <FSharpNetCoreProductDefaultTargetFramework>$([System.IO.File]::ReadAllText('$(MSBuildThisFileDirectory)../../eng/productTfm.txt').Trim())</FSharpNetCoreProductDefaultTargetFramework>
    <NoWarn>$(NoWarn);NU1603</NoWarn> 
    <!-- error NU1603: Warning As Error: Equality depends on Microsoft.NET.ILLink.Tasks 
    (>= 10.0.0-preview.7.25322.101) but Microsoft.NET.ILLink.Tasks 10.0.0-preview.7.25322.101 was not found.
     Microsoft.NET.ILLink.Tasks 10.0.0-preview.7.25380.108 was resolved instead. -->
  </PropertyGroup>
</Project>
