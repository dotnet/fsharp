      <RootNamespace>HistoricalBenchmark.Utilities</RootNamespace>
      <NoWarn>$(NoWarn);NETSDK1206</NoWarn>
    </PropertyGroup>

    <ItemGroup>
      <Compile Include="Runner.fs" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="BenchmarkDotNet" Version="$(BenchmarkDotNetVersion)" />
