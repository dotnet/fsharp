    <Tailcalls>true</Tailcalls>
    <DefineConstants>SERVICE_30_0_0</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="BenchmarkComparison.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="BenchmarkDotNet" Version="0.13.1" />
    <ProjectReference Include="..\..\..\src\fsharp\FSharp.Core\FSharp.Core.fsproj" />
    <PackageReference Include="FSharp.Compiler.Service" Version="35.0.0" />
  </ItemGroup>

</Project>
